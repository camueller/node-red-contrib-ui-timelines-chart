{
  "name": "node-red-contrib-ui-timelines-chart",
  "version": "0.0.1",
  "description": "A Node-RED widget node for showing the chart of status.",
  "author": "sanlike",
  "license": "MIT",
  "keywords": [
    "node-red",
    "dashboard",
    "status chart",
    "chart",
    "widget"
  ],
  "main": "none",
  "scripts": {
    "test": "mocha \"src/test/*_spec.js\"",
    "dev": "ts-node src/ui_timelines_chart.ts",
    "dev:watch": "ts-node-dev --respawn src/ui_timelines_chart.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "start": "node ./node_modules/node-red/red.js --userDir ./data --settings ./data/settings.js",
    "debug": "node --inspect-brk=0.0.0.0:9229 ./node_modules/node-red/red.js --userDir ./data --settings ./data/settings.js"
  },
  "node-red": {
    "version": ">=1.0.0",
    "nodes": {
      "ui_timelines_chart": "nodes/ui_timelines_chart.js"
    }
  },
  "dependencies": {
    "node-red-dashboard": "^2.30.0"
  },
  "devDependencies": {
    "@types/node-red": "^1.1.1",
    "node-red": "^2.0.3",
    "node-red-node-test-helper": "^0.2.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanlike0911/node-red-contrib-ui-timelines-chart.git"
  },
  "bugs": {
    "url": "https://github.com/sanlike0911/node-red-contrib-ui-timelines-chart/issues"
  },
  "homepage": "https://github.com/sanlike0911/node-red-contrib-ui-timelines-chart#readme"
}
